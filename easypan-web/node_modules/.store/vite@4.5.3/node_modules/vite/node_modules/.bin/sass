#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/D:/IDEA/witchPan-front/easypan-main/easypan-web/node_modules/.store/sass@1.77.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/D:/IDEA/witchPan-front/easypan-main/easypan-web/node_modules/.store/sass@1.77.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../sass@1.77.0/node_modules/sass/sass.js" "$@"
else
  exec node  "$basedir/../../../../../sass@1.77.0/node_modules/sass/sass.js" "$@"
fi
